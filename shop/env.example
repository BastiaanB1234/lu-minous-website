# Vercel Postgres Database Configuration
# Deze waarden krijg je na het aanmaken van je Postgres database op Vercel

# Hoofd database URL
DATABASE_URL="postgresql://username:password@host:port/database"

# Prisma specifieke URLs
POSTGRES_PRISMA_URL="postgresql://username:password@host:port/database?pgbouncer=true&connect_timeout=15"
POSTGRES_URL_NON_POOLING="postgresql://username:password@host:port/database?connect_timeout=15"

# Database naam
POSTGRES_DB="lu_minous_shop"

# Database host
POSTGRES_HOST="host.vercel-storage.com"

# Database poort
POSTGRES_PORT="5432"

# Database gebruiker
POSTGRES_USER="default"

# Database wachtwoord
POSTGRES_PASSWORD="your_password_here"

# Vercel specifiek
VERCEL_ENV="production"
